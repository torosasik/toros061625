          <div class="hero-images-wrapper-{{ section.id }}">
            <div class="hero-images-gradient-{{ section.id }}"></div>
            <div class="hero-images-left-{{ section.id }}">
              <div class="hero-images-inner-{{ section.id }}">
                {% for block in section.blocks %}
                  {% if forloop.index <= half_block_count %}
                    <{% if block.settings.url != blank %}a href="{{ block.settings.url }}"{% else %}div{% endif %} class="hero-image-{{ section.id }}">
                      {% if block.settings.image != blank %}
                        <img src="{{ block.settings.image | image_url }}" alt="{{ block.settings.image }}" {% if lazy %}loading="lazy"{% endif %}>
                      {% else %}
                        {{ 'image' | placeholder_svg_tag }}
                      {% endif %}
                    </{% if block.settings.url != blank %}a{% else %}div{% endif %}>
                  {% endif %}
                {% endfor %}
                {% for block in section.blocks %}
                  {% if forloop.index <= half_block_count %}
                    <{% if block.settings.url != blank %}a href="{{ block.settings.url }}"{% else %}div{% endif %} class="hero-image-{{ section.id }}">
                      {% if block.settings.image != blank %}
                        <img src="{{ block.settings.image | image_url }}" alt="{{ block.settings.image }}" {% if lazy %}loading="lazy"{% endif %}>
                      {% else %}
                        {{ 'image' | placeholder_svg_tag }}
                      {% endif %}
                    </{% if block.settings.url != blank %}a{% else %}div{% endif %}>
                  {% endif %}
                {% endfor %}
              </div>
            </div>

{% assign column_row = column_row | plus: 0 %}
{% assign column_row_tablet = column_row | times: 0.5 %}
{% assign column_row_tablet = column_row | round: 0 %}
{% assign column_row_mobile = column_row_mobile | plus: 0 %}

<script>
  function multicolumnSliderInit() {
    new Swiper('.multicolumns-slider-{{ section.id }}', {
      speed: 300,
      watchOverflow: false,
       {% if autoplay %}
       autoplay: {
         delay: {{ autoplay_speed | times: 1000 }},
       },
      {% endif %}
      navigation: {
        nextEl: '.multicolumns-btn-next-{{ section.id }}',
        prevEl: '.multicolumns-btn-prev-{{ section.id }}',
      },
      breakpoints: {
        320: {
          {% if section.blocks.size > column_row_mobile and autoplay %}
            loop: true,
          {% endif %}
          slidesPerView: {{ column_row_mobile }},
          spaceBetween: {{ column_gap_mobile }}
        },
        768: {
          {% if section.blocks.size > column_row_tablet and autoplay %}
            loop: true,
          {% else %}
            loop: false,
          {% endif %}
          slidesPerView: {{ column_row | times: 0.5 | round: 0 }},
          spaceBetween: {{ column_gap_mobile }}
        },
        992: {
          {% if section.blocks.size > column_row and autoplay %}
            loop: true,
          {% else %}
            loop: false,
          {% endif %}
          slidesPerView: {{ column_row }},
          spaceBetween: {{ column_gap}}
        }
      }
    })
  }

  document.addEventListener('DOMContentLoaded', multicolumnSliderInit);

  if (Shopify.designMode) {
     document.addEventListener('shopify:section:unload', multicolumnSliderInit);
     document.addEventListener('shopify:section:load', multicolumnSliderInit);
  }
</script>

